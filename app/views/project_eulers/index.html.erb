

<div class="container">
  <p id="notice"><%= notice %></p>

<h1>Listing Project Eulers</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_eulers.each do |project_euler| %>
      <tr>
        <td><%= project_euler.date %></td>
        <td><%= project_euler.title %></td>
        <td><%= project_euler.post %></td>
        <td><%= link_to 'Show', project_euler %></td>
        
        <% if user_signed_in? %>
          <td><%= link_to 'Edit', edit_project_euler_path(project_euler) %></td>
          <td><%= link_to 'Destroy', project_euler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<% if user_signed_in? %>
  <%= link_to 'New Project euler', new_project_euler_path %>        
<% end %>

</div>

<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>

